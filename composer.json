{
    "name": "willnewby/plaid",
    "type": "library",
    "description": "Laravel specific wrapper for Plaid. Bankdata for developers. A fork of https://github.com/travoltron/plaid ",
    "keywords": ["Travoltron", "Plaid", "Laravel", "Laravel8"],
    "homepage": "https://github.com/willnewby/plaid",
    "license": "MIT",
    "version" : "3.0.0",
    "authors": [
        {
            "name": "Ben Warburton",
            "email": "ben@travoltron.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0|~8.0",
        "guzzlehttp/guzzle": "~6.0",
        "nesbot/carbon": "~1.18"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Travoltron\\Plaid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Travoltron\\Plaid\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
